@model IEnumerable<MortgageCalculator.Dto.Mortgage>


<h1>

</h1>
<br>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MortgageType)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InterestRepayment)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EffectiveStartDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EffectiveEndDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.TermsInMonths)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.CancellationFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.EstablishmentFee)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoanAmount)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LoanDuration)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.InterestRate)
        </th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.MortgageType)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InterestRepayment)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EffectiveStartDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EffectiveEndDate)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TermsInMonths)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CancellationFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.EstablishmentFee)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoanAmount)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.LoanDuration)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.InterestRate)
            </td>
        </tr>
    }

</table>

<p>
    @Html.ActionLink("Create New", "Index")
</p>
